<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">

	<Script file="_Postmaster.lua"/>

	<Frame name="PostmasterFrame" hidden="false">
		<Scripts>
			<OnLoad>
				PostmasterFrame:RegisterAllEvents();
			</OnLoad>
			<OnEvent>
				Postmaster_OnEvent(self,event,...);
			</OnEvent>
			<OnUpdate>
				Postmaster_OnUpdate();
			</OnUpdate>
		</Scripts>
	</Frame>

</Ui>
